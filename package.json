    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "9.1.0",
    "redux": "5.0.1",
    "redux-persist": "6.0.0",
    "redux-thunk": "3.1.0",
    "reselect": "5.1.0",
    "struct-enum": "0.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",

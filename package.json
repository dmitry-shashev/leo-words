{
  "name": "leo-words",
  "version": "0.0.179",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:update": "vitest run --u",
    "test:ci": "vitest run",
    "e2e": "playwright test --ui",
    "e2e:ci": "playwright test --reporter=list",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-format": "prettier --write .",
    "generate-words": "tsx ./scripts/generate-words.ts",
    "generate-extra-img": "tsx ./scripts/generate-extra-img.ts",
    "sync": "tsx ./scripts/generate-words.ts && tsx ./scripts/generate-extra-img.ts",
    "parse-text": "tsx scripts/parse-text.ts",
    "parse-youtube": "tsx scripts/parse-youtube.ts",
    "parse-pdf": "tsx scripts/parse-pdf.ts",
    "parse-url": "tsx scripts/parse-url.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.3",
    "next": "14.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "9.1.0",
    "redux": "5.0.1",
    "redux-persist": "6.0.0",
    "redux-thunk": "3.1.0",
    "reselect": "5.1.0",
    "struct-enum": "0.0.8",
    "youtubei.js": "16.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "1.50.0",
    "@testing-library/react": "14.2.2",
    "@types/node": "^20.17.16",
    "@types/pluralize": "0.0.33",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.1.4",
    "husky": "9.0.11",
    "i": "0.3.7",
    "jsdom": "24.0.0",
    "npm": "10.5.1",
    "pdf-data-parser": "1.2.4",
    "playwright": "1.50.0",
    "pluralize": "8.0.0",
    "postcss": "^8.5.1",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.17",
    "tsx": "4.7.1",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=9.0.0"
  }
}
